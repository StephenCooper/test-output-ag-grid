    },
    {
      "name": "useBaseTwoLogAxis",
      "params": "",
      "body": "function useBaseTwoLogAxis() {options.axes = [{type: 'category',position: 'bottom'},{type: 'log',position: 'left',min: 10,label: { format: '.0f' },tick: { count: 10 },base: 2}];}"
