{
  "properties": [
    {
      "name": "options"
    }
  ],
  "externalEventHandlers": [
    {
      "name": "useNumberAxis",
      "params": "",
      "body": "function useNumberAxis() {options.subtitle = { text: 'linear scale' };options.axes[0] = {type: 'number',position: 'left',title: {enabled: true,text: 'Population'},label: {format: ',.0f',fontSize: 10}};}"
    },
    {
      "name": "useLogAxis",
      "params": "",
      "body": "function useLogAxis() {options.subtitle = { text: 'log scale' };options.axes[0] = {type: 'log',position: 'left',title: {enabled: true,text: 'Population'},label: {format: ',.0f',fontSize: 10}};}"
    }
  ],
  "instanceMethods": [],
  "globals": [],
  "init": [],
  "template": "<div class=\"wrapper\"><div id=\"toolPanel\"><button onclick=\"useNumberAxis()\">Number axis</button><span class=\"spacer\"></span><button onclick=\"useLogAxis()\">Log axis</button></div>$$CHART$$</div>"
}
