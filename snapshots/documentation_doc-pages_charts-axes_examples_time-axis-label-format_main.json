{
  "properties": [
    {
      "name": "options"
    }
  ],
  "externalEventHandlers": [
    {
      "name": "useOneMonthInterval",
      "params": "",
      "body": "function useOneMonthInterval() {options.axes[0].tick.count = agCharts.time.month;}"
    },
    {
      "name": "useTwoMonthInterval",
      "params": "",
      "body": "function useTwoMonthInterval() {options.axes[0].tick.count = agCharts.time.month.every(2);}"
    }
  ],
  "instanceMethods": [],
  "globals": [],
  "init": [],
  "template": "<div class=\"wrapper\"><div id=\"toolPanel\"><button onclick=\"useOneMonthInterval()\">Use one month interval</button><span class=\"spacer\"></span><button onclick=\"useTwoMonthInterval()\">Use two month interval</button></div>$$CHART$$</div>"
}
