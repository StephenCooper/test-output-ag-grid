{
  "properties": [
    {
      "name": "options"
    }
  ],
  "externalEventHandlers": [
    {
      "name": "setTickCountTo5",
      "params": "",
      "body": "function setTickCountTo5() {options.axes[0].tick.count = 5;options.axes[1].tick.count = 5;}"
    },
    {
      "name": "setTickCountTo10",
      "params": "",
      "body": "function setTickCountTo10() {options.axes[0].tick.count = 10;options.axes[1].tick.count = 10;}"
    }
  ],
  "instanceMethods": [],
  "globals": [
    "function generateSpiralData() {var a = 1;var b = 1;var data = [];var step = 0.1;for (var th = 1; th < 50; th += step) {var r = a + b * th;var datum = {x: r * Math.cos(th),y: r * Math.sin(th)};data.push(datum);}return data;}"
  ],
  "init": [],
  "template": "<div class=\"wrapper\"><div id=\"toolPanel\"><button onclick=\"setTickCountTo5()\">Set tick count to 5</button><span class=\"spacer\"></span><button onclick=\"setTickCountTo10()\">Set tick count to 10 (default)</button></div>$$CHART$$</div>"
}
