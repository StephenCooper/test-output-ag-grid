{
  "properties": [
    {
      "name": "options"
    }
  ],
  "externalEventHandlers": [
    {
      "name": "startUpdates",
      "params": "",
      "body": "function startUpdates() {if (updating) {return;}updating = true;this.update();setInterval(this.update, 500);}"
    }
  ],
  "instanceMethods": ["function update() {options.data = getData();}"],
  "globals": [
    "var lastTime = new Date('07 Jan 2020 13:25:00 GMT').getTime();",
    "var data = [];",
    "function getData() {data.shift();while (data.length < 20) {data.push({time: new Date(lastTime += 1000),voltage: 1.1 + Math.random() / 2});}return data;}",
    "var updating = false;"
  ],
  "init": [],
  "template": "<div class=\"wrapper\"><div id=\"toolPanel\"><button onclick=\"startUpdates()\">Start Updates</button></div>$$CHART$$</div>"
}
